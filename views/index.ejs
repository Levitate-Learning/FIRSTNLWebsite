<%- include("components/tri_image_banner", {
    image1: "home/DSC2318.jpg", 
    image2: "home/DSC2762.jpg", 
    image3: "home/IMG_3190.jpg",
    title1: "Newfoundland and Labrador's",
    title2: "premier robotics program!"
